        <script>
          function obtenerHoraArgentina() {
              const currentTime = new Date();
              const utcOffset = currentTime.getTimezoneOffset();
              const argentinaOffset = -3 * 60;
              const horaArgentina = new Date(currentTime.getTime() + (argentinaOffset + utcOffset) * 60000);
              let hh: string | number = horaArgentina.getHours();
              let mm: string | number = horaArgentina.getMinutes();
              let ss: string | number = horaArgentina.getSeconds();
              let ampm = (hh < 12) ? "AM" : "PM";
              hh = (hh % 12) || 12;
              mm = (mm < 10) ? "0" + mm : mm.toString();
              ss = (ss < 10) ? "0" + ss : ss.toString();
              const horaFormateada = hh + ":" + mm + ":" + ss + " " + ampm + " " + "GMT-3";
              document.getElementById('Reloj').textContent = horaFormateada;
          }
          
          obtenerHoraArgentina();
          
          setInterval(obtenerHoraArgentina, 1000);
          </script>
          
