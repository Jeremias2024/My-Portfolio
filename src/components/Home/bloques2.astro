  <script lang="ts">
    export function obtenerHoraArgentina() {
      const currentTime = new Date();
      const utcOffset = currentTime.getTimezoneOffset();
      const argentinaOffset = -3 * 60;
      const horaArgentina = new Date(currentTime.getTime() + (argentinaOffset + utcOffset) * 60000);
      const hh = horaArgentina.getHours();
      const mm = String(horaArgentina.getMinutes()).padStart(2, "0");
      const ss = String(horaArgentina.getSeconds()).padStart(2, "0");
      const ampm = hh < 12 ? "AM" : "PM";
      const horaFormateada = `${(hh % 12) || 12}:${mm}:${ss} ${ampm} GMT-3`;
      const relojElement = document.getElementById('Reloj');
      if (relojElement) {
      relojElement.textContent = horaFormateada;
}
    }
    setInterval(obtenerHoraArgentina, 1000);
  </script>


