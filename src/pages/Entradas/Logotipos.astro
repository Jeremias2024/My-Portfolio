---
import DefaultLayout from "../../layouts/DefaultLayout.astro";
import Titulo from '../../components/Entrys/presentacion.astro';
---

<DefaultLayout>
    <meta slot="Head" http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<meta slot="Head" http-equiv="Content-Language" content="es">
	<meta slot="Head" http-equiv="Content-Language" content="en">
	<meta slot="Head" name="viewport" content="width=device-width, initial-scale=1">
	<meta slot="Head" name="robots" content="noodp, noydir, index, follow">
	<meta slot="Head" name="description" content="Diseño y Desarrollo Web">
	<meta slot="Head" name="keywords" content="Diseño, Diseño Gráfico, Diseño Digital, Diseño Web, Desarrollo Front-End, Desarrollo Web, Programación, Portfolio, Edición de Videos, Editor, Diseñador Freelance, Desarrollador Freelancer">
	<meta slot="Head" name="author" content="Jeremías Zamora">
	<meta slot="Head" name="copyright" content="2024, Jeremías Zamora">
	<meta slot="Head" property="og:title" content="Diseño de Logotipos">
	<meta slot="Head" property="og:description" content="Diseño de logotipos y vectorizado.">
	<meta slot="Head" property="og:image" content='/Assets/Proyectos para Portfolio/Diseño Gráfico/Logos/Vanguard 2.PNG'>
	<link slot="Head" rel="icon" type="image/svg+xml" href="/favicon.png">
	<meta slot="Head" name="generator" content={Astro.generator}>
	<title slot="Head">Diseño de Logotipos</title>

    <Titulo pres= {{ 

        titulo: 'DISEÑO DE LOGOTIPOS',
        descripcion: 'Estos son algunos logotipos que he realizado tanto para mis proyectos como para amigos y clientes.',
        etiqueta1: 'Diseño y Branding',
        etiqueta2: '2022-2023',
        etiqueta3: 'Trabajos',
        imagenUrl: '/Assets/Proyectos para Portfolio/Diseño Gráfico/Logos/Vanguard 2.PNG',
        imagenAlt: 'Portada',
        imagenTitulo: 'Portada',
        
    }} />

    <div class="Triples">
        <img src='/Assets/Proyectos para Portfolio/Diseño Gráfico/Logos/Vanguard 1.PNG' alt='Logos' title='Logos' loading="lazy" />
        <img src='/Assets/Proyectos para Portfolio/Diseño Gráfico/Logos/Almohaditas mágicas isotipo.PNG' alt='Logos' title='Logos' loading="lazy" />
        <img src='/Assets/Proyectos para Portfolio/Diseño Gráfico/Logos/Almohaditas mágicas logotipo.PNG' alt='Logos' title='Logos' loading="lazy" />
        <img src='/Assets/Proyectos para Portfolio/Diseño Gráfico/Logos/Josexo Stunt.PNG' alt='Logos' title='Logos' loading="lazy" />
        <img src='/Assets/Proyectos para Portfolio/Diseño Gráfico/Logos/UMR.PNG' alt='Logos' title='Logos' loading="lazy" />
        <img src='/Assets/Proyectos para Portfolio/Diseño Gráfico/Vectorizado/Recolors de vectorizado.jpeg' alt='Logos' title='Logos' loading="lazy" />
    </div>

    <div class="C">
        <a href="/DiseñoGrafico" title="Proyectos">Volver Atrás</a>
    </div>
    
    <style>
    @font-face {
    font-family: Raleway-Regular;
    src: url('/Assets/Raleway-Regular.ttf');
}
    .C {
                display: flex;
                justify-content: center;
                margin-top: 5em;
                gap: 1.5em;
    }
    a {
                font-family: Raleway-Regular;
                color: white;
                padding: 0.8em;
                background-color: #1e1e20;
                border: solid 1px #434343;
                border-radius: 15px;
                box-shadow: inset 0px 0px 7px rgba(0, 0, 0, 0.5);
                transition: all 0.3s ease-in-out;
    }
    a:hover {
                border: solid 1px #405FA7;
                transform: scale(95%);
    }
    .Triples img {
    margin: 0 auto;
    border: solid 1px #434343;
    border-radius: 15px;
    box-shadow: inset 0px 0px 7px rgba(0, 0, 0, 0.5);
    }   
    .Triples {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 1em;
    max-width: 50em;
    margin: 0 auto;
    margin-top: 3em;
    margin-bottom: 3em;
}
    </style>
</DefaultLayout>