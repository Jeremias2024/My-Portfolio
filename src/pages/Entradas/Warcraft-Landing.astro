---
import DefaultLayout from "../../layouts/DefaultLayout.astro";
import Titulo from '../../components/Entrys/presentacion.astro';
import Cuerpo from '../../components/Entrys/explicacion.astro';
import Demo from '../../components/Entrys/Demo Grid.astro';
---

<DefaultLayout>
    <meta slot="Head" http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<meta slot="Head" http-equiv="Content-Language" content="es">
	<meta slot="Head" http-equiv="Content-Language" content="en">
	<meta slot="Head" name="viewport" content="width=device-width, initial-scale=1">
	<meta slot="Head" name="robots" content="noodp, noydir, index, follow">
	<meta slot="Head" name="description" content="Diseño y Desarrollo Web">
	<meta slot="Head" name="keywords" content="Diseño, Diseño Gráfico, Diseño Digital, Diseño Web, Desarrollo Front-End, Desarrollo Web, Programación, Portfolio, Edición de Videos, Editor, Diseñador Freelance, Desarrollador Freelancer">
	<meta slot="Head" name="author" content="Jeremías Zamora">
	<meta slot="Head" name="copyright" content="2024, Jeremías Zamora">
	<meta slot="Head" property="og:title" content="Warcraft Landing Page">
	<meta slot="Head" property="og:description" content="Presentación de Landing Page">
	<meta slot="Head" property="og:image" content='/Assets/Proyectos para Portfolio/Diseño Web/Warcraft/Warcraft Landing Page.png'>
	<link slot="Head" rel="icon" type="image/svg+xml" href="/favicon.png">
	<meta slot="Head" name="generator" content={Astro.generator}>
	<title slot="Head">Warcraft Landing Page</title>

    <Titulo pres= {{ 

        titulo: 'WARCRAFT LEGACY',
        descripcion: 'Es un proyecto ficticio que simula la presentación de un nuevo paquete de contenidos para la saga de videojuegos de Warcraft.',
        etiqueta1: 'Landing Page',
        etiqueta2: '2024',
        etiqueta3: 'Proyecto Ficticio',
        imagenUrl: '/Assets/Proyectos para Portfolio/Diseño Web/Warcraft/Warcraft Landing Page.png',
        imagenAlt: 'Warcraft Landing',
        imagenTitulo: 'Warcraft Landing',
        
    }} />

    <Cuerpo exp= {{ 

    eti: 'Idea del Proyecto',
    expli: 'La idea de esta Landing Page ficticia es presentar una nueva campaña para el juego Warcraft III. La página busca generar expectativas mostrando las características principales de la campaña, la historia desarrollada, elementos como objetos, armas y personajes con los que se podrá jugar, así como ambientes y mapas nuevos. La meta es atraer al espectador para que se suscriba a una posible beta y adquiera acceso a esta campaña.',
    
    }}/>

    <Demo demo= {{
    comentario1: 'Tecnologías Aplicadas',
    comentarioin1: 'Actualmente, el proyecto se encuentra en fase de desarrollo. El diseño ha sido creado utilizando Adobe Photoshop para maquetar y editar con mayor libertad las texturas, transparencias y fondos de pantalla.',
    imagenUrl1: '/Assets/Spaceworks/Warcraft Legacy.png',
    imagenAlt1: 'Legacy',
    imagenTitulo1: 'Legacy',
    imagenUrl2: '/Assets/Proyectos para Portfolio/Diseño Web/Warcraft/Assets/NE UI.jpg',
    imagenAlt2: 'UI',
    imagenTitulo2: 'UI',
    comentario2: 'Enfoque Conceptual',
    comentarioin2: 'La idea de este proyecto es replicar el estilo de Blizzard Entertainment al reutilizar y modificar algunos de sus elementos para recrear una interfaz de usuario similar a la de Warcraft 3 Reforged, realizando así una presentación única y atractiva.',
    imagenUrl3: '/Assets/Spaceworks/Show.png',
    imagenAlt3: 'Presentación',
    imagenTitulo3: 'Presentación',
    }} />


    <div class="C">
        <a href="/DesarrolloWeb" title="Proyectos">Volver Atrás</a>
    </div>
    
        <style>
            @font-face {
    font-family: Raleway-Regular;
    src: url('/Assets/Raleway-Regular.ttf');
}
            .C {
                display: flex;
                justify-content: center;
                margin-top: 5em;
                gap: 1.5em;
            }
            a {
                font-family: Raleway-Regular;
                color: white;
                padding: 0.8em;
                background-color: #1e1e20;
                border: solid 1px #434343;
                border-radius: 15px;
                box-shadow: inset 0px 0px 7px rgba(0, 0, 0, 0.5);
                transition: all 0.3s ease-in-out;
            }
            a:hover {
                border: solid 1px #405FA7;
                transform: scale(95%);
            }
        </style>
</DefaultLayout>